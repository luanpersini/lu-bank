{
  "name": "lu-bank",
  "version": "1.0.0",
  "author": "Luan Persini",
  "license": "ISC",
  "repository": "github:luanpersini/lu-bank",
  "description": "Portfolio - Aplicativo simulando website banc√°rio",
  "main": "index.js",
  "scripts": {
    "start": "node dist/infra/server/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/infra/server/server.js",
    "up": "npm run build && docker-compose up",
    "down": "docker-compose down",
    "dev": "ts-node-dev --respawn --transpile-only src/infra/server/server.js",
    "startd": "npm run build && npm debug",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verb": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watchAll -c jest-unit-config.js",
    "test:int": "npm test -- --watchAll -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/mongodb": "^3.6.20",
    "@types/node": "^16.3.2",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "coveralls": "^3.1.1",
    "eslint": "^7.30.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "git-commit-msg-linter": "^3.2.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-openid-connect": "^2.5.0",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.10",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-auth0": "^1.4.0",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": "14.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
